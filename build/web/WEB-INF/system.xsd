<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">
    <?xml version="1.0" encoding="UTF-8"?>
    <xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://www.uts.edu.au/31284/wsd-diary"
        xmlns="http://www.uts.edu.au/31284/wsd-diary"
        elementFormDefault="qualified">
 
        <!-- Insert your element declarations here -->
        <xs:element name="books">
            <xs:complexType>
                <xs:sequence>
                    <xs:element name="book" minOccurs="0" maxOccurs="1">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="name" type="name"/>
                                <xs:element name="introduction" type="xs:string"/>
                                <xs:element name="dob" type="date"/>
                                <xs:element name="email" type="email"/>
                                <xs:element name="interests" minOccurs="0" maxOccurs="1">
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="interest" minOccurs="0" maxOccurs="unbounded">
                                                <xs:complexType>
                                                    <xs:sequence>
                                                        <xs:element name="name" type="xs:string"/>
                                                        <xs:element name="description" type="xs:string"/>
                                                    </xs:sequence>
                                                </xs:complexType>
                                            </xs:element>
                                        </xs:sequence>
                                    </xs:complexType>
                                </xs:element>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="entry" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="date" type="date"/>
                                <xs:element name="title" type="xs:string"/>
                                <xs:element name="rating" type="rating"/>
                                <xs:element name="body" type="xs:string"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
        </xs:element>
 
    </xs:schema>
</xs:schema>
